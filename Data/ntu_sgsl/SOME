<p><script>function filterByKeyword(keyword){$("a.btn").each(function (idx, elem){const isValid=new RegExp(`.*${keyword}.*`, "i").test(elem.innerHTML); if (isValid){$(elem).parent().show();}else{$(elem).parent().hide();}});}function insertSignCount(){const count=$(".sign").length; $(".sign-count").text(`Total ${count} word(s) available to search`);}function filterByStartingLetter(letter){$("a.btn").each(function (idx, elem){const isValid=new RegExp(`^${letter}.*`, "i").test(elem.innerHTML); if (isValid){$(elem).parent().show();}else{$(elem).parent().hide();}});}$(document).ready(function (){$(document).keypress(function (e){if (e.which===47){e.preventDefault(); $("#search").focus(); $("#search").val("");}}); insertSignCount(); $("#search").on("input", function (){filterByKeyword($(this).val());}); $(".page-item.page-link").on("click", function (){const selectedElem=$(this); /* Change class styling */ $(".page-item.page-link").each(function (){$(this).removeClass("active"); if ($(this).text()===selectedElem.text()){$(this).addClass("active");}}); /* Remove filter if All Words is selected */ if (selectedElem.text()==="All Words"){filterByStartingLetter(""); $("#search-desc").text("All Words with Signs");}else{filterByStartingLetter(selectedElem.text()); $("#search-desc").text( `List of Words starting with '${selectedElem.text()}' letter` );}}); /* Remove unnecessary p tags */ $("p:empty").remove(); $(".input-group>p").replaceWith(function (){return $("#search", this);});});</script></p>
</div>
